<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:id="@+id/ID_VIEW_MSG"
    android:onClick="onClick"
    android:background="@drawable/background_ichimatsu"
    style="?android:attr/buttonBarStyle">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:background="#ffefd8">

        <FrameLayout
            android:orientation="horizontal"
            android:layout_width="1280dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            >

            <TextView
                android:id="@+id/MSG_LIMITPAGE_TITLE_TEXT"
                android:layout_width="1200dp"
                android:layout_height="260dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="60dp"
                android:layout_weight="1"
                android:background="@drawable/fs_bt_main1_corner"
                android:lines="4"
                android:paddingTop="20dp"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textIsSelectable="false"
                android:textSize="50sp"
                android:typeface="monospace" />

            <ImageView
                android:layout_width="150dp"
                android:layout_height="380dp"
                android:layout_marginLeft="1120dp"
                android:background="@drawable/ojigi"
                />

            <TextView
                android:id="@+id/MSG_LIMITPAGE_MSG_TEXT"
                android:layout_width="1100dp"
                android:layout_height="130dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="230dp"
                android:lines="4"
                android:paddingTop="20dp"
                android:text="下記のページの商品はすぐにご注文をお受けできます。"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textIsSelectable="false"
                android:textSize="40sp"
                android:textStyle="bold"
                android:typeface="monospace"
                android:visibility="invisible" />

        </FrameLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="50dp"
            android:background="#ffefd8">

            <FrameLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE1_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="80dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    android:textSize="45sp"
                    style="?android:attr/buttonBarStyle"
                    android:typeface="monospace" />

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE1"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    android:textSize="45sp"
                    android:textColor="#ffffff"
                    style="?android:attr/buttonBarStyle"
                    android:typeface="monospace" />

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE2_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="350dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE2"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="360dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE3_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="620dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp" />

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE3"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="630dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE4_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="890dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE4"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="900dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>

            </FrameLayout>

            <FrameLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE5_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="80dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    android:textSize="45sp"
                    style="?android:attr/buttonBarStyle"
                    android:typeface="monospace" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE5"
                    android:layout_width="200dp"
                    android:layout_height="80dp"
                    android:layout_marginLeft="90dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    android:textSize="45sp"
                    style="?android:attr/buttonBarStyle"
                    android:layout_gravity="center_vertical"
                    android:typeface="monospace"
                    android:textColor="#ffffff"/>
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE6_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="350dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE6"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="360dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE7_SUB"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="620dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE7"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="630dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    style="?android:attr/buttonBarStyle"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>

                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE8_SUB"
                    style="?android:attr/buttonBarStyle"
                    android:layout_width="220dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="890dp"
                    android:background="@drawable/fs_bt_main1"
                    android:onClick="onClick"
                    android:textSize="45sp" />
                <Button
                    android:id="@+id/btORDER_LIMIT_PAGE8"
                    style="?android:attr/buttonBarStyle"
                    android:layout_width="200dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="900dp"
                    android:background="@drawable/fs_bt_main1_sub"
                    android:onClick="onClick"
                    android:textSize="45sp"
                    android:textColor="#ffffff"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
            <Button
                android:layout_width="300dp"
                android:layout_height="120dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                android:background="@drawable/fs_bt_main3"
                android:textSize="50sp" />
            <Button
                android:id="@+id/btORDER_LIMIT_PAGE_BACK"
                android:layout_width="280dp"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="60dp"
                android:background="@drawable/fs_bt_main3_sub"
                android:onClick="onClick"
                android:textColor="#ffffff"
                android:textSize="50sp" />
            </FrameLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>