<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="#000000"
    style="?android:attr/buttonBarStyle"
    android:onClick="onClick">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        android:layout_marginTop="20dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_gravity="center_horizontal">

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="7"
                android:id="@+id/btNum7"
                android:background="@drawable/fs_bt_main8"
                android:textColor="#000000"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="8"
                android:id="@+id/btNum8"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="9"
                android:id="@+id/btNum9"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal">

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="4"
                android:id="@+id/btNum4"
                android:background="@drawable/fs_bt_main8"
                android:textColor="#000000"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="5"
                android:id="@+id/btNum5"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="6"
                android:id="@+id/btNum6"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal">

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="1"
                android:id="@+id/btNum1"
                android:background="@drawable/fs_bt_main8"
                android:textColor="#000000"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="2"
                android:id="@+id/btNum2"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="3"
                android:id="@+id/btNum3"
                android:background="@drawable/fs_bt_main8"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_gravity="center_horizontal">

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="0"
                android:id="@+id/btNum0"
                android:background="@drawable/fs_bt_main8"
                android:textColor="#000000"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="del"
                android:id="@+id/btNumClear"
                android:background="@drawable/fs_bt_main1"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                android:textColor="#ffffff"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick"
               />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="OK"
                android:id="@+id/btNumOk"
                android:background="@drawable/fs_bt_main4"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                android:textColor="#ffffff"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_gravity="center_horizontal"
            android:baselineAligned="true">

            <Button
                android:layout_width="250dp"
                android:layout_height="120dp"
                android:text="00"
                android:id="@+id/btNumStr"
                android:textSize="45sp"
                style="?android:attr/buttonBarStyle"
                android:background="#ffffff" />

            <Button
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:text="bk"
                android:id="@+id/btNumBack"
                android:background="@drawable/fs_bt_main3"
                android:layout_marginLeft="20dp"
                android:textSize="45sp"
                android:textColor="#ffffff"
                style="?android:attr/buttonBarStyle"
                android:onClick="onClick" />
        </LinearLayout>


    </LinearLayout>

</LinearLayout>